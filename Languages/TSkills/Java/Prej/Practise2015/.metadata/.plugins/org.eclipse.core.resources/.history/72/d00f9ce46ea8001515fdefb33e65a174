class Data{
		public void kiran(){
				System.out.println("\nHi I am Kiran....");
		}
		void shwetha(){
				System.out.println("\nHi this is Shwetha..");
		}
		private  void kandarpa(){
				System.out.println("\nHi this is Kandarpa...");
		}
}
public class Demo {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Data x=new Data();
		x.kiran();
		x.shwetha();
		x.kandarpa();
	}
}
